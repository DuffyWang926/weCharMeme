/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.indexHeader{
  margin:20rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.logoImg{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 0 20rpx 0 0;
}
.searchBox{
  flex: 1;
  display: flex;
  align-items: center;
}
.search{
  width: 100%;
  border:2rpx solid #aaa;
  height: 50rpx;
  border-radius: 50rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  flex: 1;
  
}
.searchImg{
  width: 50rpx;
  height: 50rpx;
  margin: 0 0 0 10rpx;
}
.indexCon{
  padding:20rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.imgWrapper{
  width: 45%;
  height: 400rpx;
  margin:20rpx 10rpx 0 0;
}
.conImg{
  width: 100%;
  height: 100%;
}

